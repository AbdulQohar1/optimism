generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model project {
  id Int @id @default(autoincrement())
  project_id VARCHAR(250) UNIQUE NOT NULL
  name VARCHAR(250)
  
}
